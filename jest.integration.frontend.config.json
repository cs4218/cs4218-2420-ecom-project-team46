{
  "testEnvironment": "jsdom",
  "roots": ["<rootDir>/client/src/integration-tests"],
  "testMatch": [
    "**/*.integration.test.[jt]s?(x)"
  ],
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  },
  "moduleNameMapper": {
    "\\.(css|scss)$": "identity-obj-proxy"
  },
  "transformIgnorePatterns": ["/node_modules/"],
  "silent": true,
  "setupFiles": ["<rootDir>/jest.setup.js"]
}
